{
  "name": "sql-node-delight",
  "version": "1.0.0",
  "description": "generate typesafe typescript apis from sql statements",
  "main": "index.js",
  "workspaces": [
    "app",
    "packages/*"
  ],
  "scripts": {
    "format": "deno fmt",
    "lint": "deno fmt --check && deno lint",
    "test": "vitest",
    "test:ci": "vitest run",
    "check": "tsc --noEmit",
    "build": "esbuild app/src/index.ts --outdir=build --bundle --minify --platform=node"
  },
  "devDependencies": {
    "esbuild": "0.18.15",
    "esbuild-node-tsc": "2.0.5",
    "typescript": "5.1.6",
    "vite": "4.4.4",
    "vitest": "0.33.0"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "zod": "^3.21.4"
  }
}
